{
  "name": "forza-ui",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "build": "ng build --aot --prod --vendor-chunk --common-chunk --buildOptimizer",
    "serve": "ng serve --live-reload -o",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "getVersion": "PACKAGE_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && echo $PACKAGE_VERSION",
    "postinstall": "ng build --aot --prod  --vendor-chunk --common-chunk --buildOptimizer"
  },
  "private": true,
