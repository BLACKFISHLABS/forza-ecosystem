<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="1" author="blackfish">
        <createTable tableName="APP_CHARGE">
            <column name="id" type="serial">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="created_at" type="date">
                <constraints nullable="true"/>
            </column>
            <column name="code" type="varchar(80)">
                <constraints nullable="false"/>
            </column>
            <column name="issue_date" type="date">
                <constraints nullable="false"/>
            </column>
            <column name="salesman" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="emitter" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="charge_block_id" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="vehicle_id" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="company_id" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="status" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="2" author="blackfish">
        <addForeignKeyConstraint baseColumnNames="company_id"
                                 baseTableName="APP_CHARGE" constraintName="FK_CHARGE_COMPANY"
                                 onDelete="CASCADE"
                                 onUpdate="RESTRICT"
                                 referencedColumnNames="ID"
                                 referencedTableName="APP_COMPANY"/>
    </changeSet>

    <changeSet id="3" author="blackfish">
        <createTable tableName="APP_CHARGE_BLOCK">
            <column name="id" type="serial">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="created_at" type="date">
                <constraints nullable="true"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="4" author="blackfish">
        <createTable tableName="APP_CHARGE_TABLE_PRICE">
            <column name="id" type="serial">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="created_at" type="date">
                <constraints nullable="true"/>
            </column>
            <column name="code" type="varchar(80)">
                <constraints nullable="false"/>
            </column>
            <column name="description" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="charge_block_id" type="bigint">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="5" author="blackfish">
        <createTable tableName="APP_CHARGE_ITEM">
            <column name="id" type="serial">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="created_at" type="date">
                <constraints nullable="true"/>
            </column>
            <column name="price" type="currency">
                <constraints nullable="false"/>
            </column>
            <column name="product_id" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="charge_table_price_id" type="bigint">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>


    <changeSet id="6" author="blackfish">
        <createTable tableName="APP_CHARGE_PRODUCT">
            <column name="id" type="serial">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="created_at" type="date">
                <constraints nullable="true"/>
            </column>
            <column name="code" type="varchar(80)">
                <constraints nullable="false"/>
            </column>
            <column name="ean" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="description" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="group_pro" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="note" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="qnt" type="float">
                <constraints nullable="false"/>
            </column>
            <column name="unit" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="7" author="blackfish">
        <createTable tableName="APP_CHARGE_VEHICLE">
            <column name="id" type="serial">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="created_at" type="date">
                <constraints nullable="true"/>
            </column>
            <column name="plate" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="init_km" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="end_km" type="int">
                <constraints nullable="true"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="8" author="blackfish">
        <createTable tableName="APP_CHARGE_ROUTE">
            <column name="id" type="serial">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="created_at" type="date">
                <constraints nullable="true"/>
            </column>
            <column name="charge_id" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="code" type="varchar(80)">
                <constraints nullable="false"/>
            </column>
            <column name="description" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="9" author="blackfish">
        <createTable tableName="APP_CHARGE_CUSTOMER">
            <column name="id" type="serial">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="created_at" type="date">
                <constraints nullable="true"/>
            </column>
            <column name="route_id" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="code" type="varchar(80)">
                <constraints nullable="false"/>
            </column>
            <column name="name" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="fantasy_name" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="document" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="city" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="neighborhood" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="postal_code" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="complement" type="varchar(255)">
                <constraints nullable="true"/>
            </column>
            <column name="contact" type="varchar(255)">
                <constraints nullable="true"/>
            </column>
            <column name="email" type="varchar(255)">
                <constraints nullable="true"/>
            </column>
            <column name="address" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="number" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="phone" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="secondary_phone" type="varchar(255)">
                <constraints nullable="true"/>
            </column>
            <column name="type_person" type="int">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>
